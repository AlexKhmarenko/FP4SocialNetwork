{
    "parser": "@babel-eslint/parser",
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint"
    ],
    "rules": {
      "no-trailing-spaces": ["error", { "skipBlankLines": true } ],
      "no-return-assign": 0,
      "eol-last": 0
    }
  }